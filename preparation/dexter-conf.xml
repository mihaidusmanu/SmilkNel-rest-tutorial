<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
  <models>
    <default>en</default>
    <model>
      <name>fr</name>
      <path>/data/dexter-smilk/fr.data</path>
      <sparqlendpoint>http://fr.dbpedia.org/sparql</sparqlendpoint>
      <access>local</access>
    </model>
    <model>
      <name>en</name>
      <path>/data/dexter-smilk/en.data</path>
      <sparqlendpoint>http://dbpedia.org/sparql</sparqlendpoint>
      <access>local</access>
    </model>
  </models>
  <labels>
    <dir>mapdb</dir>
  </labels>
  <index>
    <dir>lucene</dir>
    <wikiIdMap>wiki2lucene-map.bin</wikiIdMap>
  </index>
  <thresholds>
    <threshold>
      <name>commonness</name>
      <value>0.0005</value>
    </threshold>
    <threshold>
      <name>contextSimilarity</name>
      <value>0.005</value>
      <!--value>0.005</value>-->
    </threshold>
    <threshold>
      <name>linkprobability</name>
      <value>0.02</value>
    </threshold>
  </thresholds>
  <graphs>
    <dir>graph</dir>
    <graph>
      <name>entity-entity</name>
      <incoming>incoming-edges.bin</incoming>
      <outcoming>outcoming-edges.bin</outcoming>
    </graph>
    <graph>
      <name>entity-category</name>
      <incoming>incoming-entity-category-edges.bin</incoming>
      <outcoming>outcoming-entity-category-edges.bin</outcoming>
    </graph>
    <graph>
      <name>category-category</name>
      <incoming>incoming-category-edges.bin</incoming>
      <outcoming>outcoming-category-edges.bin</outcoming>
    </graph>
  </graphs>
  <rankers>
    <ranker>
      <name>commonness</name>
      <class>0.0</class>
    </ranker>
  </rankers>
  <libs>
    <lib>libs</lib>
  </libs>
  <caches>
    <cache>
      <name>relatedness</name>
      <size>5000</size>
    </cache>
    <cache>
      <name>spotter</name>
      <size>10000</size>
    </cache>
  </caches>
  <relatednessFunctions>
    <default>milnewitten</default>
    <relatednessFunction>
      <name>milnewitten</name>
      <class>it.cnr.isti.hpc.dexter.relatedness.MilneRelatedness</class>
    </relatednessFunction>
    <relatednessFunction>
      <name>milnewitten_inout</name>
      <class>fr.inria.wimmics.smilk.relatedness.MilneRelatedness_INOUT</class>
    </relatednessFunction>
    <relatednessFunction>
      <name>naive</name>
      <class>fr.inria.wimmics.smilk.relatedness.NaiveRelatedness</class>
    </relatednessFunction>
    <relatednessFunction>
      <name>pmi</name>
      <class>fr.inria.wimmics.smilk.relatedness.PMIRelatedness</class>
    </relatednessFunction>
    <relatednessFunction>
      <name>jacard</name>
      <class>fr.inria.wimmics.smilk.relatedness.JacardRelatedness</class>
    </relatednessFunction>
    <relatednessFunction>
      <name>wiki2vec</name>
      <class>fr.inria.wimmics.smilk.relatedness.wiki2vec</class>
    </relatednessFunction>
    <relatednessFunction>
      <name>ent2vecFreebase</name>
      <class>fr.inria.wimmics.smilk.relatedness.Ent2VecFreebase</class>
    </relatednessFunction>
    <relatednessFunction>
      <name>discoveryHub</name>
      <class>fr.inria.wimmics.smilk.relatedness.DiscoveryHubRelatedness</class>
    </relatednessFunction>
    <relatednessFunction>
      <name>discovery-milnewitten</name>
      <class>fr.inria.wimmics.smilk.relatedness.DiscoveryHub_MilnewittenRelatedness</class>
    </relatednessFunction>
  </relatednessFunctions>
  <similarities>
    <default>label</default>
    <similarity>
      <name>label</name>
      <labelsimilarity>levenshtein</labelsimilarity>
      <contextsimilarity>ent2vec</contextsimilarity>
    </similarity>
  </similarities>
  <labelsimilarities>
    <default>levenshtein</default>
    <labelsimilarity>
      <name>MatchLikelyhood</name>
      <class>fr.inria.wimmics.smilk.localsimilarity.MatchLikelyhoodSimilarity</class>
    </labelsimilarity>
    <labelsimilarity>
      <name>levenshtein</name>
      <class>fr.inria.wimmics.smilk.localsimilarity.LevenshteinDistance</class>
    </labelsimilarity>
  </labelsimilarities>
  <contextsimilarities>
    <contextsimilarity>
      <name>ent2vec</name>
      <class>fr.inria.wimmics.smilk.localsimilarity.Entity2VecSimilarity</class>
    </contextsimilarity>
    <contextsimilarity>
      <name>tfidf</name>
      <class>fr.inria.wimmics.smilk.localsimilarity.Entity2VecSimilarity</class>
    </contextsimilarity>
  </contextsimilarities>
  <spotRepository>
    <dir>spot</dir>
    <plainSpots>spots.tsv.gz</plainSpots>
    <perfectHash>ram/minimal-perfect-hash.bin</perfectHash>
    <offsets>ram/offsets.gz</offsets>
    <eliasFanoOffsets>ram/offsets.eliasfano.bin</eliasFanoOffsets>
    <spotsData>ram/spots.bin</spotsData>
    <entityToSpots>ram/entity-to-spots.bin</entityToSpots>
  </spotRepository>
  <ent2vecRepository>
    <dir>en2vec</dir>
    <wordModel>wiki/word.e0.100.bin</wordModel>
    <entityModel>wiki/entity.lr.e0.100.bin</entityModel>
    <entityFreebaseModel>freebase/freebase.e0.100.bin</entityFreebaseModel>
  </ent2vecRepository>
  <disambiguators>
    <default>okkam</default>
    <disambiguator>
      <name>okkam</name>
      <class>it.cnr.isti.hpc.dexter.disambiguation.TopScoreEntityDisambiguator</class>
    </disambiguator>
    <disambiguator>
      <name>wikiminer</name>
      <class>it.cnr.isti.hpc.wikiminer.Wikiminer</class>
    </disambiguator>
    <disambiguator>
      <name>pagerank</name>
      <class>fr.inria.wimmics.smilk.linking.CollectiveEntityLinking</class>
      <params>
        <param>
          <name>alpha</name>
          <value>0.4</value>
        </param>
        <param>
          <name>max-iteration</name>
          <value>100</value>
        </param>
        <param>
          <name>window-size</name>
          <value>6</value>
        </param>
        <param>
          <name>epsilon</name>
          <value>0.000001</value>
        </param>
        <param>
          <name>lambda</name>
          <value>0.1</value>
        </param>
        <param>
          <name>coherence</name>
          <value>0</value>
        </param>
      </params>
    </disambiguator>
    <disambiguator>
      <name>naivetagger</name>
      <class>fr.inria.wimmics.smilk.linking.NaiveLinking</class>
      <params>
        <param>
          <name>alpha</name>
          <value>0</value>
        </param>
        <param>
          <name>beta</name>
          <value>0.2</value>
        </param>
        <param>
          <name>gamma</name>
          <value>0.1</value>
        </param>
        <param>
          <name>window-size</name>
          <value>6</value>
        </param>
      </params>
    </disambiguator>
    <disambiguator>
      <name>linear</name>
      <class>fr.inria.wimmics.smilk.linking.LinearEntityLinking</class>
      <params>
        <param>
          <name>alpha</name>
          <value>0.43</value>
        </param>
        <param>
          <name>beta</name>
          <value>0.47</value>
        </param>
        <param>
          <name>window-size</name>
          <value>6</value>
        </param>
      </params>
    </disambiguator>
    <disambiguator>
      <name>tagme</name>
      <class>it.cnr.isti.hpc.tagme.Tagme</class>
      <params>
        <param>
          <name>window-size</name>
          <value>30</value>
        </param>
        <param>
          <name>epsilon</name>
          <value>0.7</value>
        </param>
      </params>
    </disambiguator>
    <disambiguator>
      <name>rankLib</name>
      <class>fr.inria.wimmics.smilk.linking.rankLibLabeling</class>
      <params>
        <param>
          <name>ml-model</name>
          <value>/OKE/ML/f1.train</value>
        </param>
        <param>
          <name>window-size</name>
          <value>6</value>
        </param>
      </params>
    </disambiguator>
  </disambiguators>
  <spotFilters>
    <spotFilter>
      <name>probability-filter</name>
      <class>it.cnr.isti.hpc.dexter.spotter.filter.SpotProbabilityFilter</class>
      <params>
        <param>
          <name>lp</name>
          <value>0.06</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>candidate-freq-filter</name>
      <class>it.cnr.isti.hpc.dexter.spotter.filter.CandidateEntityFrequencyFilter</class>
      <params>
        <param>
          <name>lp</name>
          <value>0.06</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>overlap-filter</name>
      <class>it.cnr.isti.hpc.dexter.spotter.filter.SpotOverlapFilter</class>
      <params>
        <param>
          <name>filter-by</name>
          <value>length</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>Spot-Overlap-Filter</name>
      <class>it.cnr.isti.hpc.dexter.spotter.filter.SpotOverlapFilter</class>
      <params>
        <param>
          <name>filter-by</name>
          <value>length</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>SpotFrequencyFilter</name>
      <class>it.cnr.isti.hpc.dexter.spotter.filter.SpotDocumentFrequencyFilter</class>
    </spotFilter>
    <spotFilter>
      <name>tfidf-similarity-filter</name>
      <class>fr.inria.wimmics.smilk.spotter.filter.EntityContextSimilarityFilter</class>
      <params>
        <param>
          <name>context-sim-threshold</name>
          <value>0</value>
        </param>
        <param>
          <name>field</name>
          <value>summary</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>ent2vec-similarity-filter</name>
      <class>fr.inria.wimmics.smilk.spotter.filter.Ent2vecSimilarityFilter</class>
      <params>
        <param>
          <name>context-sim-threshold</name>
          <value>0</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>Entity-Candidate-Filter</name>
      <class>fr.inria.wimmics.smilk.spotter.filter.EntityCandidateFilter</class>
      <params>
        <param>
          <name>max-number-of-entities</name>
          <value>15</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>POS-Tag-filter</name>
      <class>fr.inria.wimmics.smilk.spotter.filter.POSTagFilter</class>
      <params>
        <param>
          <name>threshold</name>
          <value>5</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>length-filter</name>
      <class>fr.inria.wimmics.smilk.spotter.filter.MINcharacterFilter</class>
      <params>
        <param>
          <name>length-threshold</name>
          <value>3</value>
        </param>
      </params>
    </spotFilter>
    <spotFilter>
      <name>discovery-filter</name>
      <class>fr.inria.wimmics.smilk.spotter.filter.DiscoveryHubFilter</class>
    </spotFilter>
  </spotFilters>
  <spotters>
    <spotter>
      <name>wiki-dictionary</name>
      <class>fr.inria.wimmics.smilk.spotter.DictionarySpotter</class>
      <filters>
        <filter>
          <name>SpotFrequencyFilter</name>
        </filter>
        <filter>
          <name>probability-filter</name>
        </filter>
        <filter>
          <name>POS-Tag-filter</name>
        </filter>
        <filter>
          <name>candidate-freq-filter</name>
        </filter>
        <filter>
          <name>Entity-Candidate-Filter</name>
        </filter>
        <filter>
          <name>overlap-filter</name>
        </filter>
      </filters>
      <params>
        <param>
          <name>context-window-size</name>
          <value>4</value>
        </param>
        <param>
          <name>tagging-window-size</name>
          <value>6</value>
        </param>
      </params>
    </spotter>
    <spotter>
      <name>namedEntity</name>
      <class>fr.inria.wimmics.smilk.spotter.NameEntitySpotter</class>
      <filters>
        <filter>
          <name>Spot-Overlap-Filter</name>
        </filter>
        <filter>
          <name>candidate-freq-filter</name>
        </filter>
        <filter>
          <name>tfidf-similarity-filter</name>
        </filter>
        <filter>
          <name>Entity-Candidate-Filter</name>
        </filter>
      </filters>
      <params>
        <param>
          <name>tools</name>
          <value>holmes</value>
        </param>
        <param>
          <name>context-window-size</name>
          <value>4</value>
        </param>
        <param>
          <name>tagging-window-size</name>
          <value>6</value>
        </param>
      </params>
    </spotter>
    <spotter>
      <name>namedEntity_Match</name>
      <class>fr.inria.wimmics.smilk.spotter.NameEntity_MatchingSpotter</class>
      <filters>
        <filter>
          <name>Spot-Overlap-Filter</name>
        </filter>
        <filter>
          <name>tfidf-similarity-filter</name>
        </filter>
        <filter>
          <name>Entity-Candidate-Filter</name>
        </filter>
      </filters>
      <params>
        <param>
          <name>tools</name>
          <value>stanfordwithoutcoref</value>
        </param>
        <param>
          <name>context-window-size</name>
          <value>4</value>
        </param>
        <param>
          <name>tagging-window-size</name>
          <value>6</value>
        </param>
      </params>
    </spotter>
    <spotter>
      <name>namedEntity-dbpedia</name>
      <class>fr.inria.wimmics.smilk.spotter.NerCanGenSpaqrl</class>
      <filters>
        <filter>
          <name>Spot-Overlap-Filter</name>
        </filter>
        <filter>
          <name>ent2vec-similarity-filter</name>
        </filter>
        <filter>
          <name>tfidf-similarity-filter</name>
        </filter>
        <filter>
          <name>Entity-Candidate-Filter</name>
        </filter>
      </filters>
      <params>
        <param>
          <name>tools</name>
          <value>stanfordwithoutcoref</value>
        </param>
        <param>
          <name>context-window-size</name>
          <value>4</value>
        </param>
        <param>
          <name>tagging-window-size</name>
          <value>6</value>
        </param>
      </params>
    </spotter>
    <spotter>
      <name>oketokens</name>
      <class>fr.inria.wimmics.smilk.spotter.ExtractedMentionCanGenSpaqrl</class>
      <filters>
        <filter>
          <name>ent2vec-similarity-filter</name>
        </filter>
        <filter>
          <name>tfidf-similarity-filter</name>
        </filter>
        <filter>
          <name>Entity-Candidate-Filter</name>
        </filter>
      </filters>
      <params>
        <param>
          <name>context-window-size</name>
          <value>4</value>
        </param>
        <param>
          <name>tagging-window-size</name>
          <value>6</value>
        </param>
      </params>
    </spotter>
  </spotters>
  <taggers>
    <default>dexter</default>
    <tagger>
      <name>dexter</name>
      <spotter>smilk</spotter>
      <disambiguator>wikiminer</disambiguator>
      <similarities>label</similarities>
      <relatedness>milnewitten</relatedness>
    </tagger>
    <tagger>
      <name>smilk</name>
      <spotter>wiki-dictionary</spotter>
      <disambiguator>pagerank</disambiguator>
      <similarities>label</similarities>
      <relatedness>milnewitten_inout</relatedness>
    </tagger>
  </taggers>
  <nlpmodels>
    <default>fr</default>
    <nlpmodel>
      <name>fr</name>
      <path>/nlp/fr</path>
    </nlpmodel>
    <nlpmodel>
      <name>en</name>
      <path>/nlp/en</path>
    </nlpmodel>
  </nlpmodels>
</config>
